<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TouchBehaviorIos.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:sf="clr-namespace:Syncfusion.Maui.Toolkit.SegmentedControl;assembly=Syncfusion.Maui.Toolkit"
    xmlns:pages="clr-namespace:TouchBehaviorIos.Pages"
    xmlns:xct="clr-namespace:CommunityToolkit.Maui.Behaviors;assembly=CommunityToolkit.Maui"
    Shell.FlyoutBehavior="Flyout"
    Title="TouchBehaviorIos">


    <Shell.Behaviors>
        <xct:StatusBarBehavior StatusBarColor="Red" />
    </Shell.Behaviors>

    <ShellContent
        Title="Dashboard"
        Icon="{StaticResource IconDashboard}"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="main" />


</Shell>
